AXA;AAYA,KAfA;AAgBA,IAAA,QAhBA,oBAgBA,IAhBA,EAgBA;AACA,WAAA,QAAA,GAAA,IAAA;AACA,WAAA,KAAA,GAAA,CAAA;AACA;AAnBA;AAhBA,CAAA","sourcesContent":["<template>\n  <div id=\"app\">\n    <app-bg v-bind:fill=\"bgColor\" class=\"bg\"></app-bg>\n    \n    <transition name=\"morph\">\n        <div v-if=\"state==0\" class=\"lobby card shadow-l\">\n          <button class=\"button primary\" @click=\"view='SearchGame'\"> New Game</button>\n          <button class=\"button secondary\" @click=\"view='JoinGame'\"> Join Game</button>\n          <component @results=\"createGame\" @code=\"initGame\" v-bind:is=\"view\"></component>\n        </div>\n        <play-game v-else v-bind:join-code=\"this.joinCode\" @bg=\"bgColor=$event\"></play-game>\n    </transition>\n\n  </div>\n</template>\n\n<script>\nimport \"@/assets/css/global.css\";\nimport \"@/assets/css/reset.css\";\nimport GamePlay from \"@/components/GamePlay.vue\";\nimport GameJoin from \"@/components/GameJoin.vue\";\nimport SearchBar from \"@/components/SearchBar.vue\";\nimport AppBg from \"@/components/AppBg.vue\";\n\nexport default {\n  name: 'App',\n  components: {\n    GamePlay,\n    GameJoin,\n    SearchBar,\n    AppBg,\n  },\n  data() {\n    return {\n      bgColor: \"red\",\n      state: 0,\n      view: \"\",\n      joinCode: \"\"\n    }\n  },\n  methods: {\n    createGame(list) {\n      var payload = list;\n      axios({\n        url: 'https://cors-anywhere.herokuapp.com/https://tested-quilled-regnosaurus.glitch.me/newGame',\n        method: 'post',\n        data: payload\n      })\n      .then(response => {\n        this.joinCode = response.data;\n        this.initGame(this.joinCode);\n      })\n      .catch(function (error) {\n        console.log(error);\n      });\n    },\n    initGame (code) {\n      this.joinCode = code;\n      this.state = 1;\n    },\n  }\n}\n</script>\n\n<style>\n\n\nbody,\nhtml,\n.bg,\n.flex,\n#app\n{\n  width: 100%;\n  height: 100%;\n}\n\n#app {\n  display: flex;\n  justify-content: space-around;\n  align-items: center;\n}\n\n.lobby {\n  width: 400px;\n  height: 400px;\n  display: flex;\n  justify-content: center;\n}\n\n\n.morph-enter {\n  opacity: 0;\n} \n\n.morph-enter-active {\n  transition: opacity .4s linear .6s;\n  position: absolute;\n}\n\n.morph-leave {\n  opacity: 1;\n} \n\n.morph-leave-active {\n  position: absolute;\n  animation:  scaleUp .4s;\n  animation-delay: .2s;\n  animation-fill-mode: forwards;\n\n  transition: opacity .4s linear .6s;\n  box-shadow: 0 3px 6px rgba(0, 0, 0, 0.19), 0 2px 4px rgba(0, 0, 0, 0.23);\n\n}\n\n.morph-leave-active > * {\n    opacity: 0;\n    transition: all .2s linear;\n    transform: translateY(-15px);\n}\n\n\n@keyframes scaleUp {\n  0% {\n    transform: scale();\n  }\n\n  100% {\n    transform: scaleY(1.5);\n  }\n}\n\n\n\n</style>\n"],"sourceRoot":"src"}]}